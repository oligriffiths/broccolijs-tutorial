{
  "name": "broccoli-tutorial",
  "version": "1.0.0",
  "description": "Broccoli.js tutorial application",
  "main": " ",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && broccoli build dist",
    "build-prod": "npm run clean && BROCCOLI_ENV=production broccoli build dist",
    "serve": "broccoli serve",
    "debug-build": "yarn clean && node $NODE_DEBUG_OPTION $(which broccoli) build dist",
    "debug-serve": "node $NODE_DEBUG_OPTION $(which broccoli) serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oligriffiths/broccolijs-tutorial.git"
  },
  "keywords": [
    "broccoli"
  ],
  "author": "Oli Griffiths",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oligriffiths/broccolijs-tutorial/issues"
  },
  "homepage": "https://github.com/oligriffiths/broccolijs-tutorial#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "broccoli": "^1.1.4",
    "broccoli-clean-css": "^2.0.1",
    "broccoli-env": "^0.0.1",
    "broccoli-funnel": "^2.0.1",
    "broccoli-lint-eslint": "^3.3.2",
    "broccoli-livereload": "^1.3.0",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-rollup": "^2.0.0",
    "broccoli-sass-lint": "^1.1.2",
    "broccoli-sass-source-maps": "^2.2.0",
    "broccoli-stew": "^1.5.0",
    "broccoli-uglify-sourcemap": "^2.0.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}

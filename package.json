{
  "name": "broccoli-tutorial",
  "version": "0.0.0",
  "description": "Broccoli.js tutorial application",
  "main": " ",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && broccoli build dist",
    "serve": "broccoli serve || 1",
    "debug-build":
      "npm run clean && node --nolazy --inspect-brk=5858 $NODE_DEBUG_OPTION $(which broccoli) build dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oligriffiths/broccolijs-tutorial.git"
  },
  "keywords": ["broccoli"],
  "author": "Oli Griffiths",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oligriffiths/broccolijs-tutorial/issues"
  },
  "homepage": "https://github.com/oligriffiths/broccolijs-tutorial#readme",
  "devDependencies": {
    "broccoli": "^1.1.4"
  }
}
